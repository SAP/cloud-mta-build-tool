{
  "welcomeFile": "/ui5/test/flpSandbox.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/Northwind_full_url/V2/Northwind/Northwind.svc/(.*)$",
      "target": "$1",
      "authenticationType": "none",
      "destination": "Northwind_full_url",
      "csrfProtection": false
    },
    {
      "source": "^/ui5/(.*)$",
      "target": "$1",
      "localDir": "webapp"
    }
  ]
}